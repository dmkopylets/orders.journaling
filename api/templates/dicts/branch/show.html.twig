{% extends 'dicts/base.html.twig' %}

{% block top_nav_links_right %}
    <a href="{{ path('admin_dict_branch_index') }}" class="btn btn-secondary btn-sm">back to list</a>
{% endblock %}

{% block title %}Branch{% endblock %}

{% block body %}
    <h1>Branch</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ branch.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ branch.body }}</td>
            </tr>
            <tr>
                <th>Prefix</th>
                <td>{{ branch.prefix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_dict_branch_edit', {'id': branch.id}) }}" class="btn btn-secondary btn-sm">edit</a>

    {{ include('dicts/branch/_delete_form.html.twig') }}
{% endblock %}
